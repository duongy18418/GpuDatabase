<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="205.0" prefWidth="241.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EditOptionController">
   <children>
      <Label layoutX="70.0" layoutY="14.0" text="Edit Option">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="choice" layoutX="33.0" layoutY="60.0" prefHeight="30.0" prefWidth="174.0" />
      <Button fx:id="submit" layoutX="88.0" layoutY="111.0" mnemonicParsing="false" onAction="#onSubmit" text="Submit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="95.0" layoutY="158.0" mnemonicParsing="false" onAction="#onBack" text="Return" underline="true" />
   </children>
</AnchorPane>
